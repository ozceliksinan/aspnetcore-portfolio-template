@model IEnumerable<SosyalMedya>
@{
    Layout = "_AdminLayout";
}
<br />
<div class="card card-primary">
    <div class="card-header">
        <h3 class="card-title">Sosyal Medya Ekle</h3>
    </div>
    <form asp-controller="Admin" asp-action="SosyalMedyaEkle" method="post">
        <input type="hidden" class="form-control" name="Hakkimda.HakkimdaId" readonly>
        <div class="card-body">
            <div class="form-group">
                <label>Ad</label>
                <input type="text" class="form-control" name="Ad">
            </div>
            <div class="form-group">
                <label>Link</label>
                <input type="text" class="form-control" name="Link">
            </div>
            <div class="form-group">
                <label>Icon</label>
                <input type="text" class="form-control" name="Icon">
            </div>
        </div>
        <div class="card-footer">
            <button type="submit" class="btn btn-primary">Kaydet</button>
        </div>
    </form>
</div>